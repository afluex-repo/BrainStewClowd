                        </div>
                        <div class="text-center user-info">
                            <img src="@ViewBag.Image" height="100" width="100" class="rounded-circle" alt="avatar">
                            <p class="">@ViewBag.ChildName</p>
                            <span class="text-primary">@ViewBag.LoginId</span>
                        </div>
                        <div class="user-info-list">
                            <div class="">

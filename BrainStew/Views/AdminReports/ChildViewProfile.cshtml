@model BrainStew.Models.AdminReports

@{
    ViewBag.Title = "ChildViewProfile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .user-profile .widget-content-area .user-info-list ul.contacts-block {
        max-width: 260px !important;
    }

        .user-profile .widget-content-area .user-info-list ul.contacts-block li {
            /*margin-bottom: 22px !important;*/
            margin-bottom: 39px !important;
        }

    .user-profile .widget-content-area h3 {
        position: relative;
        font-size: 30px;
        font-weight: 600;
        color: #3b3f5c;
        margin: 6px 0px 0 0;
    }
</style>





@using (Html.BeginForm("ChildViewProfile", "AdminReports", FormMethod.Post, new { @id = "form1", enctype = "multipart/form-data" }))
{
    <div class="layout-px-spacing">
        <div class="row layout-spacing">
            <!-- Content -->
            <div class="col-xl-5 col-lg-5 col-md-5 col-sm-12 layout-top-spacing">
                <div class="user-profile layout-spacing">
